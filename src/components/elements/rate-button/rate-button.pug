mixin star-rating-first-checked(diffId1, diffId2, diffId3, diffId4, diffId5)
  .rating
    input(type="checkbox" name="star" class=diffId1)
    label(for=diffId1)
    input(type="checkbox" name="star" class=diffId2)
    label(for=diffId2)
    input(type="checkbox" name="star" class=diffId3)
    label(for=diffId3)
    input(type="checkbox" name="star" class=diffId4)
    label(for=diffId4)
    input(type="checkbox" name="star" class=diffId5 checked)
    label(for=diffId5)
